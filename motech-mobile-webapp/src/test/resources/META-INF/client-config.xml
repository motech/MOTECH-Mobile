<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:jaxws="http://cxf.apache.org/jaxws"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
       http://cxf.apache.org/jaxws
       http://cxf.apache.org/schema/jaxws.xsd">

    <import resource="classpath:META-INF/omi-config.xml" />
      
    <bean id="client" class="org.motechproject.ws.mobile.MessageService"
        factory-bean="clientFactory" factory-method="create" />

    <bean id="clientFactory" class="org.apache.cxf.jaxws.JaxWsProxyFactoryBean">
        <property name="serviceClass" value="org.motechproject.ws.mobile.MessageService" />
        <property name="address" value="http://localhost:8080/motech-mobile-webapp/webservices/messaging" />
    </bean>
</beans>