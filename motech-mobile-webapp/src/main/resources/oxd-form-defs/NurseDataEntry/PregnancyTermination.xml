<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xf:xforms xmlns:xf="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" id="10">
  <xf:model>
    <xf:instance id="abortion">
      <abortion id="10" name="Preg. Termination">
        <staffId/>
        <facilityId/>
        <date/>
        <motechId/>
        <terminationType/>
        <procedure/>
        <complications/>
        <maternalDeath/>
        <referred/>
        <postAbortiobFPCounseled/>
        <postAbortionFPAccepted/>
        <comments/>
        <formtype>data_entry</formtype>
        <formname>abortion-jf</formname>
      </abortion>
    </xf:instance>
    <xf:bind id="staffId" nodeset="/abortion/staffId" required="true()" type="xsd:int" constraint=". &lt; 2147483647" message="Number too large. Keep under 2147483647"/>
    <xf:bind id="facilityId" nodeset="/abortion/facilityId" required="true()" type="xsd:int" constraint=". &lt; 2147483647" message="Number too large. Keep under 2147483647"/>
    <xf:bind constraint=". &lt;= 'today()'" id="date" message="Invalid date: date specified is in the future" nodeset="/abortion/date" required="true()" type="xsd:date"/>
    <xf:bind id="motechId" nodeset="/abortion/motechId" required="true()" type="xsd:int" constraint=". &gt; 999999 and . &lt; 10000000" message="MoTeCH ID must be 7 digits long"/>
    <xf:bind id="terminationType" nodeset="/abortion/terminationType" required="true()" type="xsd:string"/>
    <xf:bind id="procedure" nodeset="/abortion/procedure" required="true()" type="xsd:string"/>
    <xf:bind id="complications" nodeset="/abortion/complications" type="xsd:string"/>
    <xf:bind id="maternalDeath" nodeset="/abortion/maternalDeath" required="true()" type="xsd:string"/>
    <xf:bind id="referred" nodeset="/abortion/referred" required="true()" type="xsd:string"/>
    <xf:bind action="show" id="postAbortiobFPCounseled" nodeset="/abortion/postAbortiobFPCounseled" relevant="/abortion/maternalDeath = 'N'" required="true()" type="xsd:string" visible="false()"/>
    <xf:bind action="show" id="postAbortionFPAccepted" nodeset="/abortion/postAbortionFPAccepted" relevant="/abortion/postAbortiobFPCounseled = 'Y'" required="true()" type="xsd:string" visible="false()"/>
    <xf:bind id="comments" nodeset="/abortion/comments" type="xsd:string"/>
    <xf:bind id="formtype" nodeset="/abortion/formtype" required="true()" type="xsd:string" visible="false()"/>
    <xf:bind id="formname" nodeset="/abortion/formname" required="true()" type="xsd:string" visible="false()"/>
  </xf:model>
  <xf:group id="1">
    <xf:label>Page2</xf:label>
    <xf:input bind="staffId">
      <xf:label>Staff ID</xf:label>
    </xf:input>
    <xf:input bind="facilityId">
      <xf:label>Facility ID</xf:label>
    </xf:input>
    <xf:input bind="date">
      <xf:label>Termination date</xf:label>
    </xf:input>
    <xf:input bind="motechId">
      <xf:label>MoTeCH ID</xf:label>
    </xf:input>
    <xf:select1 bind="terminationType">
      <xf:label>Type</xf:label>
      <xf:item id="1">
        <xf:label>Elective</xf:label>
        <xf:value>1</xf:value>
      </xf:item>
      <xf:item id="2">
        <xf:label>Spontaneous</xf:label>
        <xf:value>2</xf:value>
      </xf:item>
      <xf:item id="3">
        <xf:label>Induced (unsafe)</xf:label>
        <xf:value>3</xf:value>
      </xf:item>
    </xf:select1>
    <xf:select1 bind="procedure">
      <xf:label>Procedure</xf:label>
      <xf:item id="NA">
        <xf:label>None</xf:label>
        <xf:value>NA</xf:value>
      </xf:item>
      <xf:item id="1">
        <xf:label>D and C</xf:label>
        <xf:value>1</xf:value>
      </xf:item>
      <xf:item id="2">
        <xf:label>MVA</xf:label>
        <xf:value>2</xf:value>
      </xf:item>
      <xf:item id="3">
        <xf:label>Other</xf:label>
        <xf:value>3</xf:value>
      </xf:item>
    </xf:select1>
    <xf:select bind="complications">
      <xf:label>Complications</xf:label>
      <xf:item id="1">
        <xf:label>Bleeding</xf:label>
        <xf:value>1</xf:value>
      </xf:item>
      <xf:item id="2">
        <xf:label>Sepsis/Infection</xf:label>
        <xf:value>2</xf:value>
      </xf:item>
      <xf:item id="3">
        <xf:label>Perforations</xf:label>
        <xf:value>3</xf:value>
      </xf:item>
    </xf:select>
    <xf:select1 bind="maternalDeath">
      <xf:label>Maternal Death</xf:label>
      <xf:item id="Y">
        <xf:label>Yes</xf:label>
        <xf:value>Y</xf:value>
      </xf:item>
      <xf:item id="N">
        <xf:label>No</xf:label>
        <xf:value>N</xf:value>
      </xf:item>
    </xf:select1>
    <xf:select1 bind="referred">
      <xf:label>Referred</xf:label>
      <xf:item id="Y">
        <xf:label>Yes</xf:label>
        <xf:value>Y</xf:value>
      </xf:item>
      <xf:item id="N">
        <xf:label>No</xf:label>
        <xf:value>N</xf:value>
      </xf:item>
    </xf:select1>
    <xf:select1 bind="postAbortiobFPCounseled">
      <xf:label>FP counseled?</xf:label>
      <xf:item id="Y">
        <xf:label>Yes</xf:label>
        <xf:value>Y</xf:value>
      </xf:item>
      <xf:item id="N">
        <xf:label>No</xf:label>
        <xf:value>N</xf:value>
      </xf:item>
    </xf:select1>
    <xf:select1 bind="postAbortionFPAccepted">
      <xf:label>FP accepted?</xf:label>
      <xf:item id="Y">
        <xf:label>Yes</xf:label>
        <xf:value>Y</xf:value>
      </xf:item>
      <xf:item id="N">
        <xf:label>No</xf:label>
        <xf:value>N</xf:value>
      </xf:item>
    </xf:select1>
    <xf:input bind="comments">
      <xf:label>Comments</xf:label>
    </xf:input>
    <xf:input bind="formtype">
      <xf:label>formType</xf:label>
    </xf:input>
    <xf:input bind="formname">
      <xf:label>formName</xf:label>
    </xf:input>
  </xf:group>
</xf:xforms>