<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xf:xforms xmlns:xf="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" id="15">
  <xf:model>
    <xf:instance id="deliverynotify">
      <deliverynotify id="15" name="Delivery Notification">
        <staffId/>
        <facilityId/>
        <motechId/>
        <datetime/>
        <formtype>data_entry</formtype>
        <formname>deliverynotify-jf</formname>
      </deliverynotify>
    </xf:instance>
    <xf:bind id="staffId" nodeset="/deliverynotify/staffId" required="true()" type="xsd:int" constraint=". &amp;lt; 2147483647" message="Number too large. Keep under 2147483647"/>
    <xf:bind id="facilityId" nodeset="/deliverynotify/facilityId" required="true()" type="xsd:int" constraint=". &amp;lt; 2147483647" message="Number too large. Keep under 2147483647"/>
    <xf:bind id="motechId" nodeset="/deliverynotify/motechId" required="true()" type="xsd:int" constraint=". &gt; 999999 and . &lt; 10000000" message="MoTeCH ID must be 7 digits long"/>
    <xf:bind constraint=". &amp;lt;= 'today()'" id="datetime" message="Invalid date: date specified is in the future" nodeset="/deliverynotify/datetime" required="true()" type="xsd:dateTime"/>
    <xf:bind id="formtype" nodeset="/deliverynotify/formtype" required="true()" type="xsd:string" visible="false()"/>
    <xf:bind id="formname" nodeset="/deliverynotify/formname" required="true()" type="xsd:string" visible="false()"/>
  </xf:model>
  <xf:group id="1">
    <xf:label>Page1</xf:label>
    <xf:input bind="staffId">
      <xf:label>Staff ID</xf:label>
    </xf:input>
    <xf:input bind="facilityId">
      <xf:label>Facility ID</xf:label>
    </xf:input>
    <xf:input bind="motechId">
      <xf:label>MoTeCH ID</xf:label>
    </xf:input>
    <xf:input bind="datetime">
      <xf:label>Date of delivery</xf:label>
    </xf:input>
    <xf:input bind="formtype">
      <xf:label>formType</xf:label>
    </xf:input>
    <xf:input bind="formname">
      <xf:label>formName</xf:label>
    </xf:input>
  </xf:group>
</xf:xforms>
