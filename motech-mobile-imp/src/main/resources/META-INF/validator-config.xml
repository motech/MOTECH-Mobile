    <bean id="dateFormatRegexMapBean" class="org.motechproject.mobile.imp.util.DateFormatRegexMap" >
        <property name="dateFormatRegexMap">
            <map>
                <entry key="(0[1-9]|[12][0-9]|3[01])/(0[1-9]|1[012])/(19|20)?\d\d" value="dd/MM/yy"/>
                <entry key="\d+" value="ddMMyy"/>
                <entry key="(0[1-9]|[12][0-9]|3[01])-(0[1-9]|1[012])-(19|20)?\d\d" value="dd-MM-yy"/>
                <entry key="(0[1-9]|[12][0-9]|3[01]).(0[1-9]|1[012]).(19|20)?\d\d" value="dd.MM.yy"/>
                <entry key="(0[1-9]|[12][0-9]|3[01]) (0[1-9]|1[012]) (19|20)?\d\d" value="dd MM yy"/>
                <entry key="(0[0-9]|1[0-2]|2[0-4]):([0-5][0-9]):([0-5][0-9]) [A|P]M$" value="hh:mm:ss a"/>
                <entry key="${imp.validator.defaultDateExpression}" value="${imp.validator.defaultDateFormat}"/>
            </map>
        </property>
    </bean>

